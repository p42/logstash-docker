#This pattern is currently not working when called from this file as it causes a timeout.
SYMPA_DO_FILE %{LOGLEVEL:log_level} %{SCOPED_FUNCTION_CALL:scoped_function} %{WORD:action} %{UNIXPATH:path} ; ([a-z]*:) %{EMAILADDRESS:email}\s\W%{PROCESSWORKER:process} ; ([\w-]+): \W%{SYMPA_MESSAGE_ID:message_id}

SYMPA_DO_MESSAGE %{LOGLEVEL:log_level} %{SCOPED_FUNCTION_CALL:function} %{GREEDYDATA:log_message}, \<%{SYMPA_MESSAGE_ID:message_id}\.%{EMAILADDRESS_OVERRIDE:sender}

SYMPA_FUNCTION [a-zA-Z]+\(\)

SYMPA_MESSAGE_ID [\h.]+

SYMPA_NO_FUNCTION %{LOGLEVEL:log_level}(?<log_message>[\s\S\W\w]+)


SYSLOGBASE %{SYSLOGTIMESTAMP:timestamp} (?:%{SYSLOGFACILITY} )?%{SYSLOGHOST:logsource}

